<view><view class="top-part"><view class="head-part"><image id="return-btn" src="https://shop.pangu.mobi/static/mobile/return.png" mode="aspectFit" data-event-opts="{{[['tap',[['goBack']]]]}}" bindtap="__e"></image><view class="head-title">我的消息</view></view></view><block s-if="{{news.length==0}}"><view class="nodata-part"><image class="nodata-img" src="https://shop.pangu.mobi/static/mobile/nodata.jpg" mode="aspectFit"></image><view class="nodata-text">暂无内容</view></view></block><block s-else><view class="news"><view class="news-inner"><block s-for="{{news}}" s-for-item="item" s-for-index="index" s-key="index"><view class="new-box"><view class="new-box-inner d-between"><view><block s-if="{{item.readed==null}}"><view class="new-box-tit">{{item.reason}}</view></block><block s-if="{{item.readed==1}}"><view class="new-box-tit readed">{{item.reason}}</view></block><view class="new-box-time">{{item.time}}</view></view><block s-if="{{item.readed==null}}"><view class="yuan"></view></block></view></view></block></view></view></block></view>