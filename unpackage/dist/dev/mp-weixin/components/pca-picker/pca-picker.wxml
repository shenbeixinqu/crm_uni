<view data-event-opts="{{[['tap',[['close',['$event']]]],['touchmove',[['',['$event']]]]]}}" class="{{['mask',(actives)?'active':'',(show)?'show':'']}}" bindtap="__e" catchtouchmove="__e"><view data-event-opts="{{[['tap',[['',['$event']]]]]}}" class="pca-picker-box" catchtap="__e"><view class="picker-head"><text>所在地区</text><text data-event-opts="{{[['tap',[['getLocation']]]]}}" class="getagain" bindtap="__e">定位到本地</text></view><view class="select-al-box"><block wx:for="{{selectAlList}}" wx:for-item="item" wx:for-index="index"><text class="{{['select-al-item',(selectAlIndex===index)?'active':'']}}" style="{{'width:'+(itemWidth+'%')+';'}}" index="{{item.title}}" data-event-opts="{{[['tap',[['selectAl',[index]]]]]}}" bindtap="__e">{{''+item.title+''}}</text></block></view><swiper class="swiper-box" current="{{currentIndex}}" data-event-opts="{{[['change',[['selectAl',['$event']]]]]}}" bindchange="__e"><block wx:for="{{pcaList}}" wx:for-item="item" wx:for-index="index"><swiper-item index="{{index}}"><scroll-view class="adress-item" scroll-into-view="{{'id'+item.target}}" scroll-y="true"><block wx:for="{{item.list}}" wx:for-item="it" wx:for-index="inx"><view class="{{['item',(inx===item.selectIndex)?'active':'']}}" index="{{it.code}}" id="{{'id'+it.code}}" data-event-opts="{{[['tap',[['selectAddress',['$0',inx,index],[[['pcaList','',index],['list','',inx]]]]]]]}}" bindtap="__e"><text class="iconfont icon-dagou-copy-copy">{{it.name}}</text></view></block></scroll-view></swiper-item></block></swiper></view></view>